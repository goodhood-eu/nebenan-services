{
  "name": "nebenan-services",
  "description": "A collection of client services",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-services",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-services/issues",
  "version": "5.0.0",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "compile": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "minify": "find ./lib -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run compile && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run compile && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.6.3",
    "axios": "^0.20.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "date-fns": "^2.6.0",
    "escape-string-regexp-browser": "^1.0.0",
    "eslint": "^7.7.0",
    "eslint-config-nebenan-base": "^1.3.1",
    "history": "^4.9.0",
    "local-storage-adapter": "^2.0.0",
    "lodash": "^4.17.15",
    "mocha": "^8.1.1",
    "nebenan-helpers": "^4.1.0",
    "nebenan-react-hocs": "^5.7.0",
    "nebenan-redux-tools": "^5.0.1",
    "normalizr": "^3.4.1",
    "qs": "^6.7.0",
    "react": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "sinon": "^9.0.3",
    "terser": "^5.2.1",
    "updeep": "^1.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.x.x",
    "local-storage-adapter": "^2.0.0",
    "updeep": "^1.1.0",
    "date-fns": "^2.x.x",
    "lodash": "^4.17.15",
    "history": "^4.9.0",
    "nebenan-helpers": "^4.x.x",
    "nebenan-redux-tools": "^4.x.x || ^5.x.x",
    "normalizr": "^3.x.x",
    "qs": "^6.x.x"
  }
}
