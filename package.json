    "babel-plugin-add-module-exports": "^1.0.4",
    "browserslist-config-nebenan": "^1.0.0",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "eslint-config-nebenan": "^4.3.0",
    "mocha": "^10.1.0",
    "redux": "^4.2.0",
    "rollup": "^3.5.1",
