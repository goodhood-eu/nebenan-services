{
  "name": "nebenan-services",
  "description": "A collection of client services",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-services",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-services/issues",
  "version": "6.0.2",
  "files": [
    "lib/*.js",
    "lib/*/*.js"
  ],
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "build": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint ./",
    "lint:tofile": "eslint --no-color -o ./eslint-report.log ./",
    "test": "npm run build && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run build && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-json-strings": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-strict-mode": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "browserslist-config-nebenan": "^1.0.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-nebenan-base": "^1.3.1",
    "lodash": "^4.17.15",
    "mocha": "^9.2.0",
    "nebenan-eventproxy": "^1.0.0",
    "nebenan-redux-tools": "^6.0.0",
    "react": "^17.0.1",
    "sinon": "^10.0.0"
  },
  "dependencies": {
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "nebenan-helpers": "^7.0.0",
    "nebenan-react-hocs": "^9.0.0",
    "normalizr": "^3.6.1",
    "qs": "^6.7.0"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.x.x",
    "nebenan-eventproxy": "^1.0.0",
    "nebenan-redux-tools": "^6.0.0"
  }
}
