  "module": "lib/index.js",
  "main": "lib/index.cjs",
  "types": "lib/types",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm test && npm run lint && npm run lint:typescript && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "build": "NODE_ENV=production rollup -c",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint ./",
    "lint:tofile": "eslint --no-color -o ./eslint-report.log ./",
    "lint:typescript": "tsc --noEmit",
    "test": "TZ='GMT' mocha",
    "test:tofile": "TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log",
    "yalc": "npm i && npm run build && yalc push"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/plugin-transform-strict-mode": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/register": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/chai": "^4.3.5",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.36",
    "@types/qs": "^6.9.7",
    "@types/sinon": "^10.0.15",
    "babel-plugin-add-module-exports": "^1.0.4",
    "browserslist-config-nebenan": "^1.0.2",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
    "eslint-config-nebenan": "^4.4.0",
    "mocha": "^10.2.0",
    "redux": "^4.2.1",
    "rollup": "^3.25.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "sinon": "^17.0.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "history": "^5.3.0",
